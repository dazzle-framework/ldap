{
    "name": "dazzlesoftware/ldap",
    "type": "library",
    "description": "Dazzle LDAP Package",
    "keywords": ["dazzle", "framework", "ldap"],
    "homepage": "https://github.com/dazzle-framework/ldap",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "ext-ldap": "*"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "dazzlesoftware/registry": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Ldap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Ldap\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
